<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="view_stock_valuation_by_vendor_mv_tree" model="ir.ui.view">
    <field name="name">stock.valuation.by.vendor.mv.tree</field>
    <field name="model">stock.valuation.by.vendor.mv</field>
    <field name="arch" type="xml">
      <tree string="Stock by Vendor">
        <field name="name"/>
      </tree>
    </field>
  </record>

  <record id="action_stock_valuation_by_vendor_mv" model="ir.actions.act_window">
    <field name="name">Stock by Vendor</field>
    <field name="res_model">stock.valuation.by.vendor.mv</field>
    <field name="view_mode">tree</field>
  </record>

  <menuitem id="menu_stock_valuation_by_vendor_mv" name="Stock by Vendor"
            parent="stock.menu_stock_root"
            action="action_stock_valuation_by_vendor_mv"/>
</odoo>