<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <!-- Inherit Form View to Modify it -->
        <record id="pos_config_view_forms_views_inheriteds" model="ir.ui.view">
            <field name="name">pos.config.view.forms.views.inheriteds</field>
            <field name="model">pos.config</field>
            <field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
            <field name="arch" type="xml">

                <xpath expr="//form/sheet/div[@id='posbox_reference']/div[@id='barcode_scanner']" position="after">
                    <!-- Add your fields or attributes here -->
                    <div id="local_system_tunneling_url_saturn" class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                        </div>
                        <div class="o_setting_right_pane">
                            <span class="o_form_label">Saturn1000</span>
                            <div class="text-muted">
                                Here you can set the local pc tunneling url for Saturn1000 pos payments terminals.
                            </div>
                            <div class="mt16">
                                <field name="local_pc_tunneling_url" style="width:100%"/>
                            </div>
                        </div>
                    </div>
                </xpath>

            </field>
        </record>


    </data>
</odoo>