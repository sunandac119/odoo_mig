<odoo>
     <data noupdate="1">

        <record id="ir_cron_close_post_pos_sessions" model="ir.cron">
            <field name="name">Auto Close - Post POS Sessions</field>
            <field name="model_id" ref="point_of_sale.model_pos_session"/>
            <field name="state">code</field>
            <field name="code">model.cron_close_post_pos_sessions()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
            <field name="nextcall" eval="(DateTime.now().replace(hour=23, minute=0, second=0)).strftime('%Y-%m-%d %H:%M:%S')"/>
        </record>

    </data>
</odoo>