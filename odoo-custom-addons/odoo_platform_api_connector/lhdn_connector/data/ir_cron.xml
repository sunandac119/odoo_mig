<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <!--         <data >-->
        <record id="ir_cron_processings_an_portal_uploaded_inv" model="ir.cron">
            <field name="name">Portal AI Invoice: Processinsg</field>
            <field name="model_id" ref="account.model_account_move"/>
            <field name="state">code</field>
            <field name="code">model.action_cron_processings_an_portal_ai_invoice()
            </field>
            <!--            <field name="user_id" ref="base.user_root"/>-->
            <field name="interval_number">3</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>
        <!--    </data>-->
        <record id="ir_cron_processings_get_notifications" model="ir.cron">
            <field name="name">LHDN: Get Notifications</field>
            <field name="model_id" ref="account.model_account_move"/>
            <field name="state">code</field>
            <field name="code">model.action_cron_processings_get_notifications()
            </field>
            <!--            <field name="user_id" ref="base.user_root"/>-->
            <field name="interval_number">3</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="active">0</field>
        </record>
        <record id="model_account_move_send_to_lhdn" model="ir.actions.server">
            <field name="name">E-Invoice Validate</field>
            <field name="model_id" ref="account.model_account_move"/>
            <field name="binding_model_id" ref="account.model_account_move"/>
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">
                if records:
                action = records.action_send_batch_for_e_invoice_validate()
            </field>
        </record>


        <record id="model_account_move_combine" model="ir.actions.server">
            <field name="name">Combine Multiple Invoices to one invoice</field>
            <field name="model_id" ref="account.model_account_move"/>
            <field name="binding_model_id" ref="account.model_account_move"/>
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">
                if records:
                action = records.action_combine_multiple_invoice_to_one_invoice()
            </field>
        </record>
        <record id="ir_cron_account_move_create_from_sftp_servers" model="ir.cron">
            <field name="name">LHDN: Creatings ans account.move usings ans SFTP Servers</field>
            <field name="model_id" ref="lhdn_connector.model_lhdn_setup"/>
            <field name="state">code</field>
            <field name="code">model.cron_create_invoice_usings_sftp()
            </field>
            <!--            <field name="user_id" ref="base.user_root"/>-->
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="active">0</field>
        </record>


        <record id="ir_cron_lhdn_myinvoice_move_fetchings" model="ir.cron">
            <field name="name">LHDN: MyInvoice Move Fetchings</field>
            <field name="model_id" ref="lhdn_connector.model_myinvoice_move"/>
            <field name="state">code</field>
            <field name="code">model.get_recent_documents_from_lhdn()
            </field>
            <!--            <field name="user_id" ref="base.user_root"/>-->
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="active">0</field>
        </record>

    </data>
</odoo>