<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_picking_form_inh" model="ir.ui.view">
        <field name="name">view.picking.form.inh</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="arch" type="xml">
            <xpath expr="//page[@name='operations']//field[@name='product_packaging_id']" position="after">
                <field name="product_packaging_qty" />
            </xpath>
        </field>
    </record>
</odoo>