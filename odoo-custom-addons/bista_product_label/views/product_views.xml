<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="0">
		<record id="action_open_label_layout" model="ir.actions.act_window">
	        <field name="name">Choose Labels Layout</field>
	        <field name="res_model">product.label.layout</field>
	        <field name="view_ids"
	                eval="[(5, 0, 0),
	                (0, 0, {'view_mode': 'form', 'view_id': ref('product_label_layout_form')})]" />
	        <field name="target">new</field>
	    </record>
		<template id="report_assets_common_inherit" inherit_id="web.report_assets_common">
			<xpath expr="." position="inside">
				<link type="text/scss" rel="stylesheet" href="/bista_product_label/static/scss/report_label_sheet.scss"/>
			</xpath>
		</template>
	</data>
</odoo>